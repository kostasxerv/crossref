{
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:mocha/recommended"],
  "env": {
    "browser": false,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true
    }],
    "no-console": "off"
  }
}